@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityUser>

<h2>Users List</h2>

<table class="table">
    <thead>
        <tr>
            <th>Email</th>
            <th>UserName</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Email</td>
                <td>@user.UserName</td>
                <td>
                    <a href="@Url.Action("EditUser", "Admin", new { id = user.Id })">Edit</a> |

                    <a href="@Url.Action("DeleteUser", "Admin", new { id = user.Id })">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
